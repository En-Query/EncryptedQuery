<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/persistence/orm http://java.sun.com/xml/ns/persistence/orm_2_0.xsd"
	version="2.0">
	
	<!-- 
		MariaDB mapping overrides 
		MariaDB does not support sequences
	-->

	<package>org.enquery.encryptedquery.querier.data.entity.jpa</package>
	
	<entity class="DataSchemaField" access="FIELD">
 		<attributes>
			<id name="id">
				<generated-value strategy="IDENTITY"/>
			</id>
		</attributes>
	</entity>
	
	<entity class="DataSchema" access="FIELD">
 		<attributes>
			<id name="id">
				<generated-value strategy="IDENTITY"/>
			</id>
		</attributes>
	</entity>

	<entity class="DataSource" access="FIELD">
 		<attributes>
			<id name="id">
				<generated-value strategy="IDENTITY"/>
			</id>
		</attributes>
	</entity>

	<entity class="Query" access="FIELD">
 		<attributes>
			<id name="id">
				<generated-value strategy="IDENTITY"/>
			</id>
		</attributes>
	</entity>
	
	<entity class="QuerySchema" access="FIELD">
 		<attributes>
			<id name="id">
				<generated-value strategy="IDENTITY"/>
			</id>
		</attributes>
	</entity>
	
	<entity class="QuerySchemaField" access="FIELD">
 		<attributes>
			<id name="id">
				<generated-value strategy="IDENTITY"/>
			</id>
		</attributes>
	</entity>
	
	<entity class="Schedule" access="FIELD">
 		<attributes>
			<id name="id">
				<generated-value strategy="IDENTITY"/>
			</id>
		</attributes>
	</entity>
	
	<entity class="Result" access="FIELD">
 		<attributes>
			<id name="id">
				<generated-value strategy="IDENTITY"/>
			</id>
		</attributes>
	</entity>
	
	<entity class="Retrieval" access="FIELD">
 		<attributes>
			<id name="id">
				<generated-value strategy="IDENTITY"/>
			</id>
		</attributes>
	</entity>
	
	<entity class="Decryption" access="FIELD">
 		<attributes>
			<id name="id">
				<generated-value strategy="IDENTITY"/>
			</id>
		</attributes>
	</entity>			
</entity-mappings>