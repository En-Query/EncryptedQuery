<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/persistence/orm http://java.sun.com/xml/ns/persistence/orm_2_0.xsd"
	version="2.0">

	<sequence-generator name="dschema_seq" sequence-name="dschema_seq" initial-value="1" allocation-size="100"/>
	<sequence-generator name="dschema_field_seq" sequence-name="dschema_field_seq" initial-value="1" allocation-size="100"/>
	<sequence-generator name="dsource_seq" sequence-name="dsource_seq" initial-value="100"/>
	<sequence-generator name="id_generator" sequence-name="ids_sequence" initial-value="1" allocation-size="100"/>
</entity-mappings>