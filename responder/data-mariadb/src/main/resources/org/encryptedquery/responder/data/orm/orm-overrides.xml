<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/persistence/orm http://java.sun.com/xml/ns/persistence/orm_2_0.xsd"
	version="2.0">
	
	<!-- 
		MariaDB mapping overrides 
		MariaDB does not support sequences
	-->

	<package>org.enquery.encryptedquery.responder.data.entity</package>
	
	<entity class="DataSchema" access="FIELD">
 		<attributes>
			<id name="id">
				<generated-value strategy="IDENTITY"/>
			</id>
		</attributes>
	</entity>
	
	<entity class="DataSchemaField" access="FIELD">
 		<attributes>
			<id name="id">
				<generated-value strategy="IDENTITY"/>
			</id>
		</attributes>
	</entity>

	<entity class="DataSourceId" access="FIELD">
 		<attributes>
			<id name="id">
				<generated-value strategy="IDENTITY"/>
			</id>
		</attributes>
	</entity>
	
	<entity class="Execution" access="FIELD">
 		<attributes>
			<id name="id">
				<generated-value strategy="IDENTITY"/>
			</id>
		</attributes>
	</entity>
	
	<entity class="Result" access="FIELD">
 		<attributes>
			<id name="id">
				<generated-value strategy="IDENTITY"/>
			</id>
		</attributes>
	</entity>
	
</entity-mappings>