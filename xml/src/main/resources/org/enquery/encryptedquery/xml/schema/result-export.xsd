<xs:schema targetNamespace="http://enquery.net/encryptedquery/result-export" 
	xmlns="http://enquery.net/encryptedquery/result-export"
	xmlns:r="http://enquery.net/encryptedquery/result"
	xmlns:ex="http://enquery.net/encryptedquery/execution"
	xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified">


	<xs:annotation>
		<xs:documentation>
			This is the XML format that is generated by Responder for import into the Querier.
			It contains results of one or more Query Executions 
			for offline mode transmission to Querier.
		</xs:documentation>
	</xs:annotation>

	<xs:import namespace="http://enquery.net/encryptedquery/result" schemaLocation="result-resource.xsd" />
	<xs:import namespace="http://enquery.net/encryptedquery/execution" schemaLocation="execution-resource.xsd" />

	<xs:element name="resultExport" type="ResultExport" />

	<xs:complexType name="ResultExport">
		<xs:sequence>
			<xs:element name="item" type="ExecutionWithResults" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>
	
	
	<xs:complexType name="ExecutionWithResults">
		<xs:sequence>
			<xs:element xmlns="http://enquery.net/encryptedquery/execution" ref="ex:executionResource"/>
			<xs:element name = "results" type="ResultCollection" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="ResultCollection">
		<xs:sequence>
			<xs:element xmlns="http://enquery.net/encryptedquery/result" ref="r:resultResource" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>

</xs:schema>